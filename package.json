{
  "name": "vue-gas",
  "version": "1.0.0",
  "dependencies": {
    "typescript": "4.9.4",
    "@types/google-apps-script": "1.0.56",

    "vue": "3.2.45",
    "vue-router": "4.1.6",
    "vuetify": "3.0.6",
    "pinia": "2.0.28",

    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "gas-webpack-plugin": "2.3.0",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "mini-css-extract-plugin": "2.7.2",
    "html-inline-script-webpack-plugin": "3.1.0",
    "html-inline-css-webpack-plugin": "1.11.1",
    "copy-webpack-plugin": "11.0.0",
    "ts-loader": "9.4.2",
    "vue-loader": "17.0.1",
    "css-loader": "6.7.3",
    "vue-style-loader": "4.1.3",
    "html-webpack-plugin": "5.5.0",
    "clean-webpack-plugin": "4.0.0",

    "@google/clasp": "2.4.2"
  },
  "devDependencies": {
    "eslint": "8.30.0",
    "prettier": "2.8.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "@typescript-eslint/parser": "5.47.1",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "eslint-plugin-vue": "9.8.0",
    "@vue/eslint-config-typescript": "11.0.2"
  },
  "scripts": {
    "init:clasp": "clasp login",
    "push:clasp": "clasp push",
    "push:build": "npm run build && npm run push:clasp",

    "build:gas": "webpack --config webpack.config.gas.js",
    "build:vue": "webpack --config webpack.config.vue.js",
    "build": "npm run build:gas && npm run build:vue",
    "watch:gas": "webpack --config webpack.config.gas.js --watch",
    "watch:vue": "webpack --config webpack.config.vue.js --watch",
    "watch": "npm run watch:gas && npm run watch:vue",

    "clasp-push": "del .\\dist\\*.txt /q && clasp push"
  }
}
